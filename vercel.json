{
  "version": 2,
  "buildCommand": "npm install --legacy-peer-deps && DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false CI=false npm run build",
  "routes": [
    {
      "src": "/api/fred",
      "dest": "/api/fred.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "FRED_API_KEY": "@fred_api_key",
    "GENERATE_SOURCEMAP": "false",
    "CI": "false",
    "DISABLE_ESLINT_PLUGIN": "true"
  }
} 